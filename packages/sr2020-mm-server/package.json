{
  "name": "sr2020-mm-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r esm -r dotenv/config --inspect ./bin/www",
    "start:server": "node -r esm -r dotenv/config ./bin/www",
    "watch:server": "nodemon --inspect -r esm -r dotenv/config ./bin/www"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.6.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "dropbox": "^4.0.30",
    "esm": "^3.2.25",
    "express": "~4.17.1",
    "express-ws": "^4.0.0",
    "http-errors": "~1.8.0",
    "isomorphic-fetch": "^3.0.0",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "shortid": "^2.2.16",
    "sr2020-mm-event-engine": "*"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "nodemonConfig": {
    "watch": [
      "app.js",
      "webSocketWrapper.js",
      "bin/",
      "routes/",
      "static/",
      "../sr2020-mm-event-engine"
    ],
    "ignore": [
      "node_modules/*"
    ]
  }
}
